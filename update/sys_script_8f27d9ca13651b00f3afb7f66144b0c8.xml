        <script><![CDATA[var instStatus = current.install_status;

// Status = Pre-Active
if (instStatus == "100" || instStatus == "200" || instStatus == "1100") {
	current.u_lifecycle = "1"; // Lifecycle = Pre-Active
}

// Status = In-Stock, In-Service, Out of Service, Removed or Unknown
else if (instStatus == "300") {
	current.u_lifecycle = "2";  // Lifecycle = Active
}

